<?xml version="1.0" encoding="UTF-8"?>
<jobs xmlns="http://www.demandware.com/xml/impex/jobs/2015-07-01">
    <job job-id="Shipped Sync Orders" priority="0">
        <description/>
        <parameters/>
        <flow>
            <context site-id="RefArch"/>
            <step step-id="syncOrders" type="ExecuteScriptModule" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="ExecuteScriptModule.Module">int_shipped/cartridge/scripts/jobs/syncOrders.js</parameter>
                    <parameter name="ExecuteScriptModule.FunctionName">execute</parameter>
                    <parameter name="ExecuteScriptModule.Transactional">false</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-recurring enabled="true">
                <recurrence>
                    <date-from>2022-10-03Z</date-from>
                    <start-time>18:20:22.000Z</start-time>
                    <interval>1m</interval>
                    <day-of-week>
                        <weekday>Thursday</weekday>
                        <weekday>Friday</weekday>
                        <weekday>Sunday</weekday>
                        <weekday>Saturday</weekday>
                        <weekday>Wednesday</weekday>
                        <weekday>Monday</weekday>
                        <weekday>Tuesday</weekday>
                    </day-of-week>
                </recurrence>
            </run-recurring>
        </triggers>
    </job>

    <job job-id="Shipped Sync Products" priority="0">
        <description/>
        <parameters/>
        <flow>
            <context site-id="RefArch"/>
            <step step-id="syncProducts" type="ExecuteScriptModule" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="ExecuteScriptModule.Module">shippedCartridge/cartridge/scripts/jobs/syncProducts.js</parameter>
                    <parameter name="ExecuteScriptModule.FunctionName">execute</parameter>
                    <parameter name="ExecuteScriptModule.Transactional">false</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-once enabled="false">
                <date>2022-09-29Z</date>
                <time>00:51:58.000Z</time>
            </run-once>
        </triggers>
    </job>
</jobs>
