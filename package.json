    "stylelint-config-standard": "^28.0.0",
    "stylelint-scss": "^4.3.0",
    "istanbul": "^0.4.4",
    "mocha": "^5.2.0",
    "sinon": "^1.17.4",
    "chai": "^3.5.0",
    "proxyquire": "1.7.4",
    "sgmf-scripts": "^2.0.0",
    "css-loader": "^6.7.1",
    "node-sass": "^7.0.3",
    "postcss-loader": "^7.0.1",
    "sass-loader": "^7.0.3"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
